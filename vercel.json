{
  "version": 2,
  "builds": [
    {
      "src": "backend/dist/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "frontend/dist/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "destination": "./backend/dist/index.js"
    },
    {
      "src": "/(.*)",
      "destination": "./frontend/dist/$1"
    }
  ]
}
